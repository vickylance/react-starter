{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "serviceworker": true
  },
  "extends": ["airbnb", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error"],
    "react/jsx-one-expression-per-line": 0,
    "react/destructuring-assignment": 0,
    "react/no-this-in-sfc": 0
  }
}